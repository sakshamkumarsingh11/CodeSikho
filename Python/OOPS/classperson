class person():
    def __init__(self, name, occupation, age):
        self.name = name
        self.occupation = occupation
        self.age = age

    def info(self):
        print(f"{self.name} is a {self.occupation} and is {self.age} years old")
a = person("Saksham", "student", 18)
b = person("xyz", "student", 18)        

a.info()
b.info()